{
  "name": "compare-directory",
  "version": "1.0.1",
  "description": "Compare contents of two directories",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && tap test/*.js",
    "lint": "eslint *.js 'lib/**/*.js' test/*.js bin/*.js",
