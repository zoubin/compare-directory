    "coveralls": "COVERALLS_REPO_TOKEN=REPO_TOKEN npm run cov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoubin/compare-directory.git"
  },
  "keywords": [
    "compare",
    "directories"
  ],
  "author": "zoubin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zoubin/compare-directory/issues"
  },
  "homepage": "https://github.com/zoubin/compare-directory#readme",
  "dependencies": {
    "globby": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^2.0.0",
    "tap": "^5.0.0"
  }
}